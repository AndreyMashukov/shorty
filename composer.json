{
    "name": "andrey-mashukov/shorty",
    "keywords": ["short", "url", "generator", "redirector"],
    "homepage": "https://github.com/AndreyMashukov/shorty",
    "description": "Short URLs redirector",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Andrey Mashukov",
            "email": "andrey@logics.net.au",
            "role": "Developer"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Logics\\Solitaire\\Shorty\\": ["classes", "classes/dao", "site", "sitecomponents"]
        }
    },
    "require": {
        "php": ">=5.6",
        "logics/init": "^0.1||dev-trunk",
        "logics/smartyextended": "^0.1||dev-trunk",
        "logics/sql": "^0.1||dev-trunk",
        "logics/web": "^0.1||dev-trunk"
    },
    "autoload-dev" : {
        "psr-4" : {
            "Logics\\Tests\\Solitaire\\Shorty\\": ["tests"]
        }
    },
    "require-dev": {
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.4",
        "phploc/phploc": "^2.1",
        "sebastian/phpcpd": "^2.0",
        "phpdocumentor/phpdocumentor": "^2.9",
        "mamuz/php-dependency-analysis": "^0.6",
        "phpunit/phpunit": "^4.8",
        "logics/phpunit-extensions": "^0.2||dev-trunk",
        "logics/codesniffer": "^0.1||dev-trunk",
        "logics/configgenerator": "^0.1||dev-trunk",
        "logics/daogenerator": "^0.1||dev-trunk",
        "logics/gettextgenerator": "^0.1||dev-trunk",
        "logics/htaccessgenerator": "^0.1||dev-trunk",
        "logics/mapsgenerator": "^0.1||dev-trunk",
        "logics/sitecomposer": "^0.1||dev-trunk",
        "logics/http": "^0.1||dev-trunk"
    },
    "prefer-stable": true
}
